<?xml version="1.0" encoding="UTF-8" ?>
<ISUI>
	<Text Name='lblScoutCoin1' Template='EQ2.text'>
		<X>10</X>
		<Y>10</Y>
		<Width>145</Width>
		<Height>20</Height>
		<Alignment>Left</Alignment>
		<Text>Coin 1</Text>
	</Text>
	<combobox name='cbScoutCoin1' Template='EQ2.Combobox'>
		<x>150</x>
		<y>10</y>
		<width>240</width>
		<height>20</height>
		<OnLeftClick>
			declare Counter int
			Counter:Set[1]
			This:ClearItems
			do
			{
				if ${Me.Ability[${Counter}].HOIconID}==41
				{
					This:AddItem[${Me.Ability[${Counter}].Name}]
				}
			}
			while ${Counter:Inc}&lt;=${Me.NumAbilities}
		</OnLeftClick>
		<OnLoad>
			This:AddItem[${SettingXML[Scripts/EQ2Bot/Character Config/${Me.Name}.xml].Set[EQ2BotExtras].Set[HeroicOp].GetString[ScoutCoin1]}]
			This.ItemByValue[${SettingXML[Scripts/EQ2Bot/Character Config/${Me.Name}.xml].Set[EQ2BotExtras].Set[HeroicOp].GetString[ScoutCoin1]}]:Select
		</OnLoad>
		<OnSelect>
			Script[EQ2Bot].Variable[ScoutCoin1]:Set[${This.SelectedItem.Text}]
			SettingXML[Scripts/EQ2Bot/Character Config/${Me.Name}.xml].Set[EQ2BotExtras].Set[HeroicOp]:Set[ScoutCoin1,${This.SelectedItem.Text}]:Save
		</OnSelect>
	</combobox>
