			<Button Name="m_wndOkButton" VerticalAlignment="Center" Height="21.816" Width="89.082" IsDefault="True" IsCancel="True" Click="m_wndOkButton_Click" Canvas.Left="138.168" Canvas.Bottom="10.908">OK</Button>
