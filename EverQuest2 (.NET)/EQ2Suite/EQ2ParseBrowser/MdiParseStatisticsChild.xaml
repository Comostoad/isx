<UserControl x:Class="EQ2ParseBrowser.MdiParseStatisticsChild"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Height="316" Width="675">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*" MinWidth="20" />
			<ColumnDefinition Width="3*" MinWidth="20" />
		</Grid.ColumnDefinitions>
		<TreeView Grid.Row="0" Grid.Column="0">

		</TreeView>
		<GridSplitter Width="5" />
		<ListView SelectionMode="Single"  Grid.Row="0" Grid.Column="1">
			<ListView.View>
				<GridView>
					<GridViewColumn Width="300" Header="ASDF"></GridViewColumn>
				</GridView>
			</ListView.View>
		</ListView>
	</Grid>
</UserControl>
